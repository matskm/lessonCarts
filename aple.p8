pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
--a snake game

cls()

x = 60 y = 60

going_lt = false
going_rt = false
going_up = false
going_dn = false

psprite = 001

function update_for_direc_change()

  if(btn(0)) then
    psprite = 001 
    going_lt = true
    
    going_rt = false
    going_up = false
    going_dn = false
  end
  
  if(btn(1)) then
    psprite = 002 
    going_rt = true
    
    going_lt = false
    going_up = false
    going_dn = false
  end
  
  if(btn(2)) then
    psprite = 003 
    going_up = true
    
    going_rt = false
    going_lt = false
    going_dn = false
  end
  
  if(btn(3)) then
    psprite = 004 
    going_dn = true
    
    going_rt = false
    going_up = false
    going_lt = false
  end
  
end


function update_player_pos()

  if(going_lt) then
    x = x-1
  end
  
  if(going_rt) then
    x = x+1
  end
  
  if(going_up) then
    y = y-1
  end
  
  if(going_dn) then
    y = y+1
  end

end


function _update()
 update_for_direc_change()

 update_player_pos()
end

function _draw()
 cls()
 spr((psprite),x,y) 
 spr(005,70,40)
end
